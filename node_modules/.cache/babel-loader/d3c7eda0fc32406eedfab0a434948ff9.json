\" />\r\n    <button @click=\"serach\">搜索</button>\r\n    <ul>\r\n      <li\r\n        v-for=\"item in SerachValue ? newcityMEList : cityMEList\"\r\n        :key=\"item.id\"\r\n      >\r\n        <span v-if=\"SerachValue\" v-html=\"item\"></span>\r\n        <span v-else>{{ item.comtent }}</span>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import axios from 'axios'\r\nexport default {\r\n  data() {\r\n    return {\r\n      SerachValue: \"\",\r\n      newcityMEList: [],\r\n      cityMEList: [],\r\n    };\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    // 发起请求\r\n    serach() {\r\n      fetch(\"./data.json\")\r\n        .then((res) => res.json())\r\n        .then((data) => {\r\n          console.log(data);\r\n        });\r\n\r\n      if (!this.SerachValue) return;\r\n      this.newcityMEList = [];\r\n      this.cityMEList.forEach((item) => {\r\n        if (item.name.includes(this.SerachValue)) {\r\n          let itemdata = JSON.parse(JSON.stringify(item));\r\n          let dataArr = itemdata.name.split(this.SerachValue);\r\n          let str = dataArr.join(\r\n            `<span style=\"color:red\">${this.SerachValue}</span>`\r\n          );\r\n          this.newcityMEList.push(str);\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\nul,\r\nli {\r\n  margin: 0;\r\n  padding: 0;\r\n  list-style: none;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}